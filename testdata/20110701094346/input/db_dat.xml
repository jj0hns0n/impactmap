  lon		CDATA 			#REQUIRED
  source	(SCSN|CDMG|NSMP) 	'SCSN'
  commtype	(DIG|ANA) 		'DIG'
>

<!ELEMENT comp (acc,vel,psa*)>
<!ATTLIST comp
